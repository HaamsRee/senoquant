{
  "name": "stardist_onnx",
  "description": "StarDist ONNX model wrapper using upstream StarDist preprocessing/tiling",
  "version": "0.1.0",
  "tasks": {
    "nuclear": {
      "supported": true
    },
    "cytoplasmic": {
      "supported": false
    }
  },
  "settings": [
    {
      "key": "prob_thresh",
      "label": "Probability threshold",
      "type": "float",
      "decimals": 2,
      "min": 0.0,
      "max": 1.0,
      "default": 0.5
    },
    {
      "key": "nms_thresh",
      "label": "NMS threshold",
      "type": "float",
      "decimals": 2,
      "min": 0.0,
      "max": 1.0,
      "default": 0.4
    },
    {
      "key": "n_tiles",
      "label": "Tiles per axis",
      "type": "int",
      "min": 1,
      "max": 16,
      "default": 1
    }
  ]
}
